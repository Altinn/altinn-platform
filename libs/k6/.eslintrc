{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "globals": {
        "__ENV": "readonly" // From K6
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "arrow-body-style": "warn",
        "camelcase": 0,
        "object-curly-newline": 0,
        "operator-linebreak": 0,
        "no-shadow": 0,
        "max-len": [
            2,
            120
        ],
        "no-underscore-dangle": "off",
        "no-unused-vars": "off"
    }
}