{
    "executor": "ramping-arrival-rate",
    "stages": [
        {
            "duration": "10m",
            "target": 100
        }
    ],
    "thresholds": {
        "http_req_failed": [
            {
                "threshold": "rate==0.0",
                "abortOnFail": true,
                "delayAbortEval": "10s"
            }
        ],
        "http_req_duration": [
            {
                "threshold": "max<5000",
                "abortOnFail": true,
                "delayAbortEval": "10s"
            }
        ]
    }
}